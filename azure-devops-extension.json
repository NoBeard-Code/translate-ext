{
    "manifestVersion": 1,
    "id": "azure-translator-api-extension",
    "publisher": "FactoryX",
    "version": "1.0.7",
    "name": "Azure Translator API Action Button",
    "description": "A configurable extension that provides an action button for translating fields by using Azure AI Translator API",
    "public": false,
    "categories": ["Azure Boards"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "static/azure-translator.jpeg"
    },
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "scopes": [
        "vso.work"
    ],
    "tags": [
        "action button",
        "translation",
        "Azure AI"
    ],
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ]
}